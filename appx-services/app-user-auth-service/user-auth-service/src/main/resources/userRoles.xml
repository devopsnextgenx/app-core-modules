<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user_role_SYSTEM_ADMINISTRATOR"
          class="io.devopsnextgenx.microservices.modules.models.Role">
        <property name="name" value="SYSTEM_ADMINISTRATOR"/>
        <property name="externalId" value="ROLE-0000"/>
    </bean>

    <bean id="user_role_USER"
          class="io.devopsnextgenx.microservices.modules.models.Role">
        <property name="name" value="USER"/>
        <property name="externalId" value="ROLE-0001"/>
    </bean>
    <bean id="user_role_TEACHER"
          class="io.devopsnextgenx.microservices.modules.models.Role">
        <property name="name" value="TEACHER"/>
        <property name="externalId" value="ROLE-0002"/>
    </bean>
    <bean id="user_role_STUDENT"
          class="io.devopsnextgenx.microservices.modules.models.Role">
        <property name="name" value="STUDENT"/>
        <property name="externalId" value="ROLE-0003"/>
    </bean>
    <bean id="user_role_ACCOUNTS"
          class="io.devopsnextgenx.microservices.modules.models.Role">
        <property name="name" value="ACCOUNTS"/>
        <property name="externalId" value="ROLE-0004"/>
    </bean>
</beans>